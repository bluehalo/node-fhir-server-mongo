<% if(meta && meta.tag) { %>
<h5 class="mt-4">Bundle Meta</h5>
<table class="table">
  <thead>
    <tr>
      <th scope="col">System</th>
      <th scope="col">Code/Display</th>
    </tr>
  </thead>
  <% meta.tag.forEach(function(tag){ %>
  <tr>
    <td><%= tag.system %></td>
    <td><%= tag.code || tag.display %></td>
  </tr>
  <% }) %>
  <tbody></tbody>
</table>
<% } %>
