<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
  <%- include('../partials/header', {resources: resources}); %>
</header>

<main>
  <div class="jumbotron">
    <% resources.forEach(function(res){ %>
        <%- include('../partials/resourceHeader', {res: res}); %>

        <h4>Active</h4><%= res.active %>

        <%- include('../partials/reference', {references: [res.organization], name: "Organization"}); %>
        <%- include('../partials/reference', {references: [res.participatingOrganization], name: "Participating Organization"}); %>


        <%- include('../partials/json', {res: res}); %>
    <%})%>
  </div>
</main>

<footer>
    <%- include('../partials/bundleMeta', {meta: meta}); %>
    <%- include('../partials/footer', {url: url}); %>
</footer>

</body>
</html>
